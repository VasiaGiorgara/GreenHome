import RPi.GPIO as gp  
from time import sleep  
gp.setmode(gp.BOARD)  
gp.setup(12,gp.OUT)  
pwm=gp.PWM(12,20)  
pwm.start(0)  
for i in range(0,181):  
    sig=(i/8)+2  
    pwm.ChangeDutyCycle(sig)  
    sleep(0.02)  
for i in range(180,-1,-1):  
    sig=(i/8)+2  
    pwm.ChangeDutyCycle(sig)  
    sleep(0.02)  
pwm.stop()  
gp.cleanup()  
